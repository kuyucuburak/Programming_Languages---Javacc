PROGRAM xorxnor;

DECLARATION SECTION
-- Declare variables
P, Q, R;

INITIALIZATION SECTION
-- Initialize the variables
Q=TRUE;
P=UNKNOWN;
R=FALSE;

MAIN SECTION
-- Compute the XOR of P and Q
INPUT 'Enter P:' P;
INPUT 'Enter Q:' Q;
R = NOT (P AND Q OR NOT P AND NOT Q);
OUTPUT 'P XOR Q is:' R;
OUTPUT 'P XNOR Q is:' ((P AND Q) OR (NOT P AND NOT Q));